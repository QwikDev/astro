{
  "name": "qwikdev-astro",
  "version": "0.0.0",
  "scripts": {
    "check": "pnpm lint && pnpm fmt.check",
    "fix": "pnpm fmt && pnpm lint.fix",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx,astro}\"",
    "lint.fix": "eslint --fix \"**/*.{js,jsx,ts,tsx,astro}\"",
    "lint.dry-run": "eslint --fix-dry-run \"**/*.{js,jsx,ts,tsx,astro}\""
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.3",
    "eslint-plugin-qwik": "^1.4.2",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "typescript": "5.3.3"
  }
}
