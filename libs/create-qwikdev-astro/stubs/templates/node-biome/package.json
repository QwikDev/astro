{
  "name": "@qwikdev/astro-node-biome-starter",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "build": "npm run fix && astro build",
    "check": "biome ci . && astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "astro dev",
    "fix": "npm run lint && npm run format",
    "format": "biome format --write .",
    "lint": "biome check --apply-unsafe .",
    "preview": "npm run build && npm run serve",
    "prod": "npm run check && astro build",
    "serve": "node ./dist/server/entry.mjs",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.0.0",
    "@builder.io/qwik": "^1.11",
    "@qwikdev/astro": "https://pkg.pr.new/QwikDev/astro/@qwikdev/astro@6d96827",
    "astro": "5.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0"
  }
}
