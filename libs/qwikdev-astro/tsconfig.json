{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "outDir": "./dist",
    "moduleResolution": "Bundler",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "baseUrl": ".",
